{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-dupe-keys": 0,
    "no-case-declarations": 0,
    "no-unused-vars": 0,
    "react/react-in-jsx-scope": "off",
    "react-hooks/exhaustive-deps": 0,
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "plugins": [
    "react-hooks",
    "react",
    "prettier"
  ]
}